<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="575.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.moritzklein.spigotserverlauncher.viewcontroller.MainController">
   <children>
      <TextArea fx:id="taServerOutput" layoutX="14.0" layoutY="45.0" prefHeight="485.0" prefWidth="675.0" />
      <TextField fx:id="tfCommand" layoutX="14.0" layoutY="536.0" prefHeight="27.0" prefWidth="535.0" />
      <Button fx:id="btnStartStop" layoutX="705.0" layoutY="536.0" mnemonicParsing="false" onAction="#startServerStopServer" prefHeight="27.0" prefWidth="131.0" text="Start" />
      <Button fx:id="btnSend" layoutX="559.0" layoutY="536.0" mnemonicParsing="false" onAction="#sendCommand" prefHeight="27.0" prefWidth="131.0" text="Send" />
      <MenuBar prefHeight="29.0" prefWidth="850.0">
        <menus>
          <Menu mnemonicParsing="false" text="Server">
            <items>
                  <MenuItem mnemonicParsing="false" text="Start" />
              <MenuItem mnemonicParsing="false" text="Stop" />
                  <MenuItem mnemonicParsing="false" text="Use external Server Jar" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Console">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#clearConsole" text="Clear Console" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
